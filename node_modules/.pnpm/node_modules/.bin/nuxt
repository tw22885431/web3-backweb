#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/web/web3-backweb/node_modules/.pnpm/nuxi@3.13.1/node_modules/nuxi/bin/node_modules:/mnt/d/web/web3-backweb/node_modules/.pnpm/nuxi@3.13.1/node_modules/nuxi/node_modules:/mnt/d/web/web3-backweb/node_modules/.pnpm/nuxi@3.13.1/node_modules:/mnt/d/web/web3-backweb/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/web/web3-backweb/node_modules/.pnpm/nuxi@3.13.1/node_modules/nuxi/bin/node_modules:/mnt/d/web/web3-backweb/node_modules/.pnpm/nuxi@3.13.1/node_modules/nuxi/node_modules:/mnt/d/web/web3-backweb/node_modules/.pnpm/nuxi@3.13.1/node_modules:/mnt/d/web/web3-backweb/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nuxi/bin/nuxi.mjs" "$@"
else
  exec node  "$basedir/../nuxi/bin/nuxi.mjs" "$@"
fi
