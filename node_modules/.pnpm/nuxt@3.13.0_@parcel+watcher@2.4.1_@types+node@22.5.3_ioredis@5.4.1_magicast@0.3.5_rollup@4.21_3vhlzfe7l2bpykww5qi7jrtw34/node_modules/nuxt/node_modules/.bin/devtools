#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/data/data/com.termux/files/home/web3-shop-backweb/node_modules/.pnpm/@nuxt+devtools@1.4.1_rollup@4.21.2_vite@5.4.3_@types+node@22.5.3_terser@5.31.6_/node_modules/@nuxt/devtools/node_modules:/data/data/com.termux/files/home/web3-shop-backweb/node_modules/.pnpm/@nuxt+devtools@1.4.1_rollup@4.21.2_vite@5.4.3_@types+node@22.5.3_terser@5.31.6_/node_modules/@nuxt/node_modules:/data/data/com.termux/files/home/web3-shop-backweb/node_modules/.pnpm/@nuxt+devtools@1.4.1_rollup@4.21.2_vite@5.4.3_@types+node@22.5.3_terser@5.31.6_/node_modules:/data/data/com.termux/files/home/web3-shop-backweb/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/data/data/com.termux/files/home/web3-shop-backweb/node_modules/.pnpm/@nuxt+devtools@1.4.1_rollup@4.21.2_vite@5.4.3_@types+node@22.5.3_terser@5.31.6_/node_modules/@nuxt/devtools/node_modules:/data/data/com.termux/files/home/web3-shop-backweb/node_modules/.pnpm/@nuxt+devtools@1.4.1_rollup@4.21.2_vite@5.4.3_@types+node@22.5.3_terser@5.31.6_/node_modules/@nuxt/node_modules:/data/data/com.termux/files/home/web3-shop-backweb/node_modules/.pnpm/@nuxt+devtools@1.4.1_rollup@4.21.2_vite@5.4.3_@types+node@22.5.3_terser@5.31.6_/node_modules:/data/data/com.termux/files/home/web3-shop-backweb/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../@nuxt/devtools/cli.mjs" "$@"
else
  exec node  "$basedir/../../../@nuxt/devtools/cli.mjs" "$@"
fi
